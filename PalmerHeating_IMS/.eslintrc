{
  "extends": ["@meteorjs/eslint-config-meteor", "plugin:prettier/recommended"],
  "rules": {
    "jsx-a11y/label-has-for": false,
    "no-console": 0,
    "consistent-return": 0,
    "no-plusplus": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-underscore-dangle": 0,
    "prettier/prettier": "error"
  },
  "plugins": ["prettier"],
  "env": {
    "jest": true
  }
}
